  "compilerOptions": {
    "esModuleInterop": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "noEmit": true,
    "strictNullChecks": true,
    "target": "esnext",
    "jsx": "react"
  },
  "include": [
    "client/js/**/*",
    "server/**/*"
  ]
}
